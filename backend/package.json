{
    "name": "devtrack-backend",
    "version": "0.1.0",
    "private": true,
    "main": "dist/server.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc -p tsconfig.json",
        "start": "node dist/server.js",
        "test:build": "tsc -p tsconfig.test.json",
        "test": "npm run test:build && node --test dist-test/test/**/*.test.js"
    },
    "dependencies": {
        "bcrypt": "^5.1.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.6",
        "dotenv": "^16.0.0",
        "express": "^4.18.2",
        "express-validator": "^7.0.0",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^7.0.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/cookie-parser": "^1.4.10",
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^20.0.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4.2"
    }
}
